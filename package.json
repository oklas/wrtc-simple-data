{
  "name": "wrtc-simple-data",
  "version": "0.0.2",
  "description": "A simple Node.js library to communicate via WebRTC data channels as simple and lightweight as any possible.",
  "main": "dist/src",
  "typings": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint src/**/*.ts",
    "test": "jest",
    "cover": "jest --coverage",
    "example:client": "babel-node --presets es2015 -- dist/examples/commandline-client.js"
  },
  "dependencies": {
    "freeice": "^2.2.0",
    "socket.io-client": "^1.4.5",
    "wrtc": "^0.0.62"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n1try/wrtc-simple-data.git"
  },
  "keywords": [
    "webrtc",
    "datachannel",
    "node"
  ],
  "author": "Ferdinand MÃ¼tsch <mail@ferdinand-muetsch.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n1try/wrtc-simple-data/issues"
  },
  "homepage": "https://github.com/n1try/wrtc-simple-data#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.12",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.0",
    "jest-cli": "^20.0.4",
    "portfinder": "^1.0.13",
    "rimraf": "^2.6.1",
    "socket.io": "^2.0.3",
    "ts-jest": "^20.0.7",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  }
}
